{
  "compileOnSave": false,
  "compilerOptions": {
    "types": ["jest", "lodash","gapi","gapi.auth2","gapi.client"],
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "allowJs": true,
    "lib": [
      "es2018",
      "dom"
    ]
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "allowJs": true,
    "strictInjectionParameters": true
  },
  "types":["googlemaps"]
}